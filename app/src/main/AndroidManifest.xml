<?xml version="1.0" encoding="utf-8"?>
<manifest package="learn.maoy.com.rxtest"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="learn.maoy.com.rxtest.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="learn.maoy.com.rxtest.NextActivity">
        </activity>
        <activity android:name="learn.maoy.com.rxtest.news.NewsActivity">
        </activity>
        <activity android:name="learn.maoy.com.rxtest.rxandroid.MyRxActivity">
        </activity>
    </application>

</manifest>